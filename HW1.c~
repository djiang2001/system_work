#include <stdio.h>
#include <stdlib.h>

int mult_3_5(){
  int sum = 0;
  int count = 0;
  while (count != 1000) {
    if (count % 3 == 0 || count % 5 == 0) {
      sum += count;
    }
    count++;
  }
  return sum;
}
int main() {
  printf("Solution for number one: %d \n",mult_3_5());
	return 0;
}
